{
  "name": "@midcurve/semsee-core",
  "version": "0.1.0",
  "description": "SEMSEE Core Orchestrator - Strategy execution engine for the embedded EVM",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "semsee-node": "./dist/main.js",
    "semsee": "./dist/cli/index.js"
  },
  "scripts": {
    "start": "tsx --env-file=.env src/main.ts",
    "dev": "tsx --env-file=.env watch src/main.ts",
    "build": "tsc",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "cli": "tsx --env-file=.env src/cli/index.ts"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "hyperliquid": "^1.5.0",
    "pino": "^9.6.0",
    "viem": "^2.38.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/ws": "^8.18.1",
    "pino-pretty": "^13.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
